{
  "[ruby]": {
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": true,
  "debug.inlineValues": true,
  "editor.fontFamily": "Operator Mono Lig Book",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.snippetSuggestions": "top",
  "editor.wordWrap": "wordWrapColumn",
  "emmet.excludeLanguages": ["markdown", "php"],
  "files.associations": {
    "*.jsonc": "jsonc",
    ".env.*": "dotenv",
    "bashrc": "shellscript",
    "profile": "shellscript"
  },
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "go.inferGopath": true,
  "go.toolsGopath": "~/go",
  "logViewer.watch": [
    {
      "pattern": "var/log/*.log",
      "title": "Symfony"
    }
  ],
  "namespaceResolver.sortAlphabetically": true,
  "namespaceResolver.sortNatural": true,
  "python.linting.pylintUseMinimalCheckers": false,
  "ruby.codeCompletion": "rcodetools",
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.lint": {
    "rubocop": true
  },
  "ruby.useLanguageServer": true,
  "rust.mode": "rls",
  "rust.rls": {
    "useRustfmt": true
  },
  "rust.rustup": {
    "nightlyToolchain": "nightly-x86_64-unknown-linux-gnu",
    "toolchain": "stable-x86_64-unknown-linux-gnu"
  },
  "terminal.integrated.fontFamily": "Fira code",
  "terminal.integrated.fontSize": 12,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Codecourse Contrast (rainglow)",
  "workbench.iconTheme": "material-icon-theme",
  "namespaceResolver.leadingSeparator": true,
  "php.suggest.basic": false
}
