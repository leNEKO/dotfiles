{
  "[ruby]": {
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": true,
  "debug.inlineValues": true,
  "editor.fontFamily": "Operator Mono Lig Book",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "top",
  "editor.rulers": [
    80,
    120
  ],
  "emmet.excludeLanguages": [
    "markdown",
    "php"
  ],
  "emmet.includeLanguages": {
    "twig": "html",
    "smarty": "html"
  },
  "files.associations": {
    "*.jsonc": "jsonc",
    ".env.*": "dotenv",
    "bashrc": "shellscript",
    "profile": "shellscript",
  },
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "namespaceResolver.sortAlphabetically": true,
  "namespaceResolver.sortNatural": true,
  "terminal.integrated.fontFamily": "Operator Mono Lig Book",
  "terminal.integrated.fontSize": 12,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": -1,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "statusBar.background": "#000",
    "statusBar.foreground": "#DDD",
    "statusBar.noFolderBackground": "#333",
    "statusBar.noFolderForeground": "#fff"
  },
  "namespaceResolver.leadingSeparator": true,
  "php.suggest.basic": false,
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.views.compare.location": "scm",
  "gitlens.views.search.location": "scm",
  "php.validate.enable": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "terminal.integrated.rendererType": "dom",
  "debug.console.fontFamily": "Operator Mono Lig Book",
  "gitlens.views.repositories.files.layout": "tree",
  "task.autoDetect": "off",
  "editor.highlightActiveIndentGuide": false,
  "bracket-pair-colorizer-2.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 2px",
    "borderColor : {color}",
    "opacity: 1"
  ],
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "kite.showWelcomeNotificationOnStartup": false,
  "phpcs.executablePath": "/home/neko/.composer/vendor/bin/phpcs",
  "phpcs.showSources": true,
  "intelephense.environment.phpVersion": "7.2.0",
  "local-history.absolute": true,
  "vscode-php-cs-fixer.rules": "@PSR1,@PSR2,@Symfony,-yoda_style,-new_line_for_chained_calls",
  "vscode-php-cs-fixer.config": ".php_cs",
  "timeline.excludeSources": []
}
